{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.png",
          "/*.svg",
          "/*.webmanifest",
          "/logo.*",
          "/manifest.webmanifest",
          "/site.webmanifest"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "pdf-cache",
      "urls": ["/**/*.pdf"],
      "cacheConfig": {
        "maxSize": 30,
        "maxAge": "30d",
        "strategy": "performance"
      }
    }
  ]
}
