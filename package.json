{
  "name": "Pdf Annotator",
  "version": "1.3.0",
  "author": "AAmoros",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:local": "node scripts/start-local.mjs",
    "start:docker": "bash scripts/docker-up.sh",
    "build": "npm run i18n:check && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:pdf-annotator": "node dist/pdf-annotator/server/server.mjs",
    "i18n:check": "node scripts/check-translations.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "19.2.0",
    "@angular/compiler": "19.2.0",
    "@angular/core": "19.2.0",
    "@angular/forms": "19.2.0",
    "@angular/material": "19.2.0",
    "@angular/platform-browser": "19.2.0",
    "@angular/platform-server": "19.2.0",
    "@angular/router": "19.2.0",
    "@angular/service-worker": "19.2.0",
    "@angular/ssr": "19.2.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "@angular/cdk": "19.2.0",
    "@pdf-lib/fontkit": "1.1.1",
    "express": "^5.1.0",
    "pdf-lib": "1.17.1",
    "pdfjs-dist": "5.4.296",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "19.2.0",
    "@angular/cli": "19.2.0",
    "@angular/compiler-cli": "19.2.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "@types/pdfjs-dist": "^2.10.377",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}